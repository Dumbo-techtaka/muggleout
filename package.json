{
  "name": "muggleout",
  "version": "1.1.3",
  "description": "Transform muggles into terminal wizards / 비개발자를 터미널 마법사로 만들어주는 도구",
  "main": "src/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dumbo-techtaka/muggleout.git"
  },
  "homepage": "https://github.com/Dumbo-techtaka/muggleout#readme",
  "bugs": {
    "url": "https://github.com/Dumbo-techtaka/muggleout/issues"
  },
  "bin": {
    "muggleout": "./bin/muggleout.js"
  },
  "scripts": {
    "start": "node bin/muggleout.js",
    "dev": "node --watch bin/muggleout.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:version": "node bin/muggleout.js --version",
    "test:quick": "./scripts/quick-test.sh",
    "pre-release-check": "node scripts/pre-release-check.js",
    "prepublishOnly": "npm run pre-release-check && npm run test:version && npm test"
  },
  "keywords": [
    "cli",
    "setup",
    "developer-tools",
    "terminal",
    "beginner-friendly",
    "korean",
    "macos",
    "homebrew",
    "iterm2",
    "oh-my-zsh",
    "natural-language",
    "dev-tools"
  ],
  "author": "dumbokim <dumbo@techtaka.com> (rlawlsgus97@gmail.com)",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "execa": "^8.0.1",
    "inquirer": "^9.2.12",
    "natural": "^6.10.4",
    "node-emoji": "^2.1.3",
    "ora": "^8.0.1",
    "semver": "^7.5.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "pkg": {
    "scripts": "src/**/*.js",
    "assets": [
      "src/i18n/locales/**/*"
    ],
    "targets": [
      "node18-macos-x64",
      "node18-macos-arm64",
      "node18-linux-x64",
      "node18-win-x64"
    ],
    "outputPath": "dist"
  },
  "files": [
    "bin/",
    "src/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ]
}
